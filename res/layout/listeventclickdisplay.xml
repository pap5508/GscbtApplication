<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:background="@drawable/event_title"
		android:id="@+id/linearLayout1" android:layout_height="wrap_content"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_width="fill_parent" android:orientation="horizontal">
		<Button android:drawableLeft="@drawable/home"
			android:background="@null" android:textColor="#ffffff" android:id="@+id/eventclick_home"
			android:layout_height="wrap_content" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:layout_marginBottom="5dip"
			android:layout_weight="1" android:layout_width="0dp"></Button>
		<TextView android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_height="wrap_content" android:id="@+id/textView4"
			android:text=" " android:layout_weight="1" android:layout_width="0dp"></TextView>
		<Button android:drawableRight="@drawable/back_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/eventclick_back"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:layout_marginRight="10dip" android:layout_marginTop="10dip"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/ll1"
		android:orientation="horizontal" android:layout_below="@+id/linearLayout1"
		android:layout_alignParentLeft="true" android:layout_marginTop="10dip">
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_weight="1"
			android:layout_width="0dp" android:textStyle="bold" android:text="Event Title"></TextView>
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:editable="false"
			android:layout_weight="1" android:layout_width="0dp"
			android:textSize="14px" android:textAppearance="?android:attr/textAppearanceLarge"
			android:id="@+id/text_1"></TextView>
	</LinearLayout>
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:id="@+id/ll2" android:layout_height="wrap_content"
		android:layout_below="@+id/ll1" android:layout_marginTop="10dp">
		<TextView android:text="Approve" android:textColor="#000000"
			android:layout_width="0dp" android:layout_height="wrap_content"
			android:layout_weight="1" android:textStyle="bold"></TextView>
		<TextView android:id="@+id/text_2" android:textColor="#000000"
			android:layout_width="0dp" android:textSize="14px"
			android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:editable="false">
		</TextView>
	</LinearLayout>
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/ll3"
		android:layout_below="@+id/ll2" android:layout_marginTop="10dp">
		<TextView android:text="Start Date" android:textColor="#000000"
			android:layout_width="0dp" android:layout_height="wrap_content"
			android:layout_weight="1" android:textStyle="bold"></TextView>
		<TextView android:id="@+id/text_3" android:textColor="#000000"
			android:layout_width="0dp" android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_height="wrap_content" android:textSize="14px"
			android:editable="false" android:layout_weight="1"></TextView>
	</LinearLayout>
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:id="@+id/ll4" android:layout_height="wrap_content"
		android:layout_below="@+id/ll3" android:layout_alignParentLeft="true"
		android:layout_marginTop="10dp">
		<TextView android:text="End Date" android:textColor="#000000"
			android:layout_width="0dp" android:layout_height="wrap_content"
			android:layout_weight="1" android:textStyle="bold"></TextView>
		<TextView android:id="@+id/text_4" android:textColor="#000000"
			android:layout_width="0dp" android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_weight="1" android:textSize="14px"
			android:layout_height="wrap_content" android:layout_alignParentLeft="true"
			android:editable="false"></TextView>
	</LinearLayout>
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:id="@+id/ll5" android:layout_height="wrap_content"
		android:layout_below="@+id/ll4" android:layout_alignParentLeft="true"
		android:layout_marginTop="10dp">
		<TextView android:text="Description" android:layout_width="0dp"
			android:textColor="#000000" android:layout_height="wrap_content"
			android:layout_weight="1" android:textStyle="bold"></TextView>
		<TextView android:id="@+id/text_5" android:layout_width="0dp"
			android:textAppearance="?android:attr/textAppearanceLarge"
			android:textColor="#000000" android:layout_weight="1"
			android:textSize="14px" android:layout_height="100dp"
			android:layout_alignParentLeft="true" android:editable="false"></TextView>
	</LinearLayout>
</RelativeLayout>
