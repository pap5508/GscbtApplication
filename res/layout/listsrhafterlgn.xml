<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/linearLayout1" android:layout_height="fill_parent"
	android:layout_width="fill_parent" android:orientation="vertical">
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/linearLayout2" android:layout_width="fill_parent"
		android:baselineAligned="false">
		<TextView android:layout_height="wrap_content" android:text="BloodBank"
			android:id="@+id/text1" android:layout_width="0dp" android:textColor="#000000"
			android:layout_marginLeft="1dip" android:layout_marginRight="2dip"
			android:layout_weight="1"></TextView>
		<TextView android:layout_height="wrap_content" android:text="WholeBlood"
			android:id="@+id/text2" android:layout_width="wrap_content"
			android:textColor="#000000" android:layout_marginLeft="25dip"
			android:layout_marginRight="45dip"></TextView>
		<ImageButton android:layout_height="32px"
			android:layout_width="32px" android:id="@+id/search_map" android:src="@drawable/map"
			android:background="@null" android:layout_gravity="center"
			android:layout_marginLeft="1dip" android:layout_marginRight="2dip"></ImageButton>
		<ImageButton android:layout_height="32px"
			android:layout_width="32px" android:id="@+id/search_detail"
			android:src="@drawable/info" android:background="@null"
			android:layout_gravity="center" android:layout_marginLeft="1dip"
			android:layout_marginRight="2dip"></ImageButton>
		<ImageButton android:layout_height="32px"
			android:layout_width="32px" android:id="@+id/search_msg" android:src="@drawable/sendmsg"
			android:background="@null" android:layout_gravity="center"
			android:layout_marginLeft="1dip" android:layout_marginRight="2dip"></ImageButton>


	</LinearLayout>


</LinearLayout>