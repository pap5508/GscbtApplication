<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:gravity="top">
	<LinearLayout android:layout_width="fill_parent"
		android:background="@drawable/stock_update_title"
		android:layout_height="wrap_content" android:id="@+id/linearLayout1"
		android:orientation="horizontal" android:layout_alignParentTop="true"
		android:layout_alignParentLeft="true">
		<Button android:id="@+id/stockclick_home" android:drawableLeft="@drawable/home"
			android:layout_height="wrap_content" android:textColor="#ffffff"
			android:layout_marginBottom="5dip" android:layout_weight="1"
			android:layout_width="0dp" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:background="@null"></Button>
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_weight="1"
			android:layout_width="0dp" android:textAppearance="?android:attr/textAppearanceMedium"
			android:id="@+id/textView4" android:text=" "></TextView>
		<Button android:id="@+id/stockclick_back"
			android:layout_marginRight="10dip" android:layout_height="wrap_content"
			android:textColor="#ffffff" android:layout_weight="1"
			android:layout_width="0dp" android:layout_marginTop="10dip"
			android:drawableRight="@drawable/back_icon" android:background="@null"></Button>
	</LinearLayout>

	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:id="@+id/ll2" android:layout_height="wrap_content"
		android:layout_below="@+id/ll1" android:layout_marginLeft="10dip"
		android:layout_marginRight="10dip">
		<TextView android:text="BloodBank" android:layout_width="0dp"
			android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_height="wrap_content" android:layout_weight="1"></TextView>
		<EditText android:id="@+id/text_2" android:layout_width="0dp"
			android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_height="wrap_content" android:layout_weight="1"></EditText>
	</LinearLayout>

	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/ll3"
		android:layout_below="@+id/ll2" android:layout_marginLeft="10dip"
		android:layout_marginRight="10dip">
		<TextView android:text="BloodGroup" android:layout_width="0dp"
			android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_height="wrap_content" android:layout_weight="1"></TextView>
		<EditText android:id="@+id/text_3" android:layout_width="0dp"
			android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_height="wrap_content" android:layout_weight="1"></EditText>
	</LinearLayout>

	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:id="@+id/ll4" android:layout_height="wrap_content"
		android:layout_below="@+id/ll3" android:layout_marginLeft="10dip"
		android:layout_marginRight="10dip">
		<TextView android:text="Stock" android:layout_width="0dp"
			android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_height="wrap_content" android:layout_weight="1"></TextView>
		<EditText android:id="@+id/text_4" android:layout_width="0dp"
			android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_height="wrap_content" android:layout_weight="1"></EditText>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/ll1"
		android:orientation="horizontal" android:layout_below="@+id/linearLayout1"
		android:layout_alignParentLeft="true" android:layout_marginLeft="10dip"
		android:layout_marginRight="10dip" android:layout_marginTop="10dip">
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_weight="1"
			android:layout_width="0dp" android:textAppearance="?android:attr/textAppearanceLarge"
			android:text="Id"></TextView>
		<EditText android:layout_height="wrap_content"
			android:layout_weight="1" android:layout_width="0dp" android:id="@+id/text_1"
			android:textAppearance="?android:attr/textAppearanceLarge">
			<requestFocus></requestFocus>
		</EditText>
	</LinearLayout>


</RelativeLayout>
