<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:background="@drawable/registration_title"
		android:id="@+id/linearLayout1" android:layout_height="wrap_content"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_width="fill_parent" android:orientation="horizontal">
		<Button android:drawableLeft="@drawable/home"
			android:background="@null" android:textColor="#ffffff" android:id="@+id/reg_homebb"
			android:layout_height="wrap_content" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:layout_marginBottom="5dip"
			android:layout_weight="1" android:layout_width="0dp"></Button>
		<TextView android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_height="wrap_content" android:id="@+id/textView4"
			android:text=" " android:layout_weight="1" android:layout_width="0dp"></TextView>
		<Button android:drawableRight="@drawable/back_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/donor_back"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:layout_marginRight="10dip" android:layout_marginTop="10dip"></Button>
	</LinearLayout>
	<ScrollView android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@+id/scroll_regbb"
		android:layout_alignParentLeft="true" android:layout_below="@+id/linearLayout1"
		android:layout_above="@+id/linearLayout2" android:layout_marginLeft="10dip"
		android:layout_marginRight="10dip">
		<LinearLayout android:id="@+id/ll2_regbb"
			android:orientation="vertical" android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			<LinearLayout android:id="@+id/ll4_regbb"
				android:orientation="vertical" android:layout_width="fill_parent"
				android:layout_height="fill_parent">
				<LinearLayout android:layout_width="fill_parent"
					android:id="@+id/ll3_regbb" android:layout_height="wrap_content">
					<TextView android:layout_weight="1" android:layout_height="wrap_content"
						android:textColor="#000000" android:layout_gravity="center"
						android:layout_width="0dp" android:text="UserType"
						android:textStyle="bold" android:id="@+id/tv_utypebb"></TextView>
					<EditText android:layout_weight="1" android:layout_height="40dp"
						android:text="BloodBank" android:layout_width="0dp"
						android:editable="false" android:id="@+id/reg_utypebb"></EditText>
				</LinearLayout>
				<TextView android:text="BloodBank Information" android:id="@+id/tv_personalbb"
					android:textColor="#000000" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textStyle="bold"
					android:textSize="16px"></TextView>
				<LinearLayout android:id="@+id/ll5_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="BloodBankname *"
						android:textColor="#000000" android:id="@+id/tv_namebb"
						android:layout_width="0dp" android:layout_height="wrap_content"
						android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_namebb"
						android:layout_weight="1">
						<requestFocus></requestFocus>
					</EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll6_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Username *" android:id="@+id/tv_unamebb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_unamebb"
						android:layout_weight="1"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll7_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Password *" android:id="@+id/tv_pwdbb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_pwdbb"
						android:layout_weight="1" android:inputType="textPassword"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll8_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Confirm Password *" android:id="@+id/tv_cpwdbb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_cpwdbb"
						android:layout_weight="1" android:inputType="textPassword"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll9_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Bloodbank Category *" android:id="@+id/tv_categorybb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/reg_spinnercatbb"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll10_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Bloodbank SubCategory *"
						android:textColor="#000000" android:id="@+id/tv_subcatbb"
						android:layout_width="0dp" android:layout_height="wrap_content"
						android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/reg_spinnersubcatbb"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll11_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="NACO Approved *" android:id="@+id/tv_nacobb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/reg_spinnernacobb"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll12_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Licence Number *" android:id="@+id/tv_licnobb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_licnobb"
						android:layout_weight="1"></EditText>
				</LinearLayout>


			</LinearLayout>
			<LinearLayout android:id="@+id/ll13_regbb"
				android:layout_marginTop="5dp" android:orientation="vertical"
				android:layout_width="fill_parent" android:layout_height="fill_parent">
				<TextView android:text="Contact Information" android:id="@+id/tv_contactbb"
					android:textColor="#000000" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textStyle="bold"
					android:textSize="16px"></TextView>

				<LinearLayout android:id="@+id/ll14_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Address *" android:id="@+id/tv_addbb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_addbb"
						android:layout_weight="1" android:inputType="textMultiLine"></EditText>
				</LinearLayout>

				<LinearLayout android:id="@+id/ll15_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="City" android:id="@+id/tv_citybb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/reg_spinnercitybb"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>

				<LinearLayout android:id="@+id/ll16_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="State" android:id="@+id/tv_statebb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_statebb"
						android:layout_weight="1"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll17_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Zip Code" android:id="@+id/tv_zipbb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_zipbb"
						android:layout_weight="1" android:inputType="number"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll18_reg"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Phone Number *" android:id="@+id/tv_phbb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_phbb"
						android:layout_weight="1" android:inputType="number"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll19_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="E-mail" android:id="@+id/tv_eidbb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_eidbb"
						android:layout_weight="1" android:inputType="textEmailAddress"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll20_regbb"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Web Site" android:id="@+id/tv_sitebb"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/reg_sitebb"
						android:layout_weight="1"></EditText>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
	<LinearLayout android:background="@drawable/footer_title"
		android:id="@+id/linearLayout2" android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true" android:layout_width="fill_parent">
		<Button android:drawableTop="@drawable/ok_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/reg_submitbb"
			android:text="SUBMIT" android:layout_height="wrap_content"
			android:layout_width="0dp" android:layout_gravity="bottom"></Button>
		<Button android:drawableTop="@drawable/reset_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/reg_cancelbb"
			android:text="RESET" android:layout_height="wrap_content"
			android:layout_width="0dp" android:layout_gravity="bottom"></Button>
	</LinearLayout>

</RelativeLayout>
