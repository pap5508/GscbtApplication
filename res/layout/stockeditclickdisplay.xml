<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:gravity="top">
	<LinearLayout android:layout_width="fill_parent"
		android:background="@drawable/stock_detail_title"
		android:layout_height="wrap_content" android:id="@+id/linearLayout1"
		android:orientation="horizontal" android:layout_alignParentTop="true"
		android:layout_alignParentLeft="true">
		<Button android:id="@+id/stockclick_home" android:drawableLeft="@drawable/home"
			android:layout_height="wrap_content" android:textColor="#ffffff"
			android:layout_marginBottom="5dip" android:layout_weight="1"
			android:layout_width="0dp" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:background="@null"></Button>
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_weight="1"
			android:layout_width="0dp" android:textAppearance="?android:attr/textAppearanceMedium"
			android:id="@+id/textView4" android:text=" "></TextView>
		<Button android:id="@+id/stockclick_back"
			android:layout_marginRight="10dip" android:layout_height="wrap_content"
			android:textColor="#ffffff" android:layout_weight="1"
			android:layout_width="0dp" android:layout_marginTop="10dip"
			android:drawableRight="@drawable/back_icon" android:background="@null"></Button>
	</LinearLayout>


	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/linearLayout2" android:orientation="vertical"
		android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true" android:layout_width="fill_parent">
		<Button android:drawableTop="@drawable/ok_icon"
			android:background="@null" android:id="@+id/stockclick_update"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:textColor="#000000" android:layout_gravity="center"
			android:text="UPDATE"></Button>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/linearLayout3" android:orientation="vertical"
		android:layout_below="@+id/linearLayout1"
		android:layout_alignParentLeft="true" android:layout_width="fill_parent"
		android:layout_above="@+id/linearLayout2" android:layout_marginLeft="10dip"
		android:layout_marginRight="10dip" android:layout_marginTop="10dip">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:id="@+id/linearLayout3"
			android:orientation="horizontal" android:layout_alignParentTop="true"
			android:layout_alignParentLeft="true">
			<TextView android:text="User Id" android:layout_width="0dp"
				android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceLarge"
				android:layout_height="wrap_content" android:layout_weight="1"></TextView>
			<EditText android:id="@+id/text_1" android:textAppearance="?android:attr/textAppearanceLarge"
				android:layout_height="wrap_content" android:editable="false"
				android:layout_width="0dp" android:layout_weight="1"></EditText>
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:id="@+id/linearLayout3"
			android:orientation="horizontal" android:layout_alignParentTop="true"
			android:layout_alignParentLeft="true" android:layout_marginTop="5dp">
			<TextView android:text="WholeBlood" android:layout_width="0dp"
				android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceLarge"
				android:layout_height="wrap_content" android:layout_weight="1"></TextView>
			<EditText android:id="@+id/text_2" android:textAppearance="?android:attr/textAppearanceLarge"
				android:layout_height="wrap_content" android:layout_width="0dp"
				android:layout_weight="1">
				<requestFocus></requestFocus>
			</EditText>
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:id="@+id/linearLayout3"
			android:orientation="horizontal" android:layout_alignParentTop="true"
			android:layout_alignParentLeft="true" android:layout_marginTop="5dp">
			<TextView android:text="PacketCount" android:layout_width="0dp"
				android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceLarge"
				android:layout_height="wrap_content" android:layout_weight="1"></TextView>
			<EditText android:id="@+id/text_3" android:textAppearance="?android:attr/textAppearanceLarge"
				android:layout_height="wrap_content" android:layout_width="0dp"
				android:layout_weight="1"></EditText>
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:id="@+id/linearLayout3"
			android:orientation="horizontal" android:layout_alignParentTop="true"
			android:layout_alignParentLeft="true" android:layout_marginTop="5dp">
			<TextView android:text="FreshFrozen" android:layout_width="0dp"
				android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceLarge"
				android:layout_height="wrap_content" android:layout_weight="1"></TextView>
			<EditText android:id="@+id/text_4" android:textAppearance="?android:attr/textAppearanceLarge"
				android:layout_height="wrap_content" android:layout_width="0dp"
				android:layout_weight="1"></EditText>

		</LinearLayout>

	</LinearLayout>
</RelativeLayout>
