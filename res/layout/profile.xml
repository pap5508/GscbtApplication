<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:background="@drawable/profile_title"
		android:id="@+id/linearLayout1" android:layout_height="wrap_content"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_width="fill_parent" android:orientation="horizontal">
		<Button android:drawableLeft="@drawable/home"
			android:background="@null" android:textColor="#ffffff" android:id="@+id/profile_home"
			android:layout_height="wrap_content" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:layout_marginBottom="5dip"
			android:layout_weight="1" android:layout_width="0dp"></Button>
		<TextView android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_height="wrap_content" android:id="@+id/textView4"
			android:text=" " android:layout_weight="1" android:layout_width="0dp"></TextView>
		<Button android:drawableRight="@drawable/back_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/patient_back"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:layout_marginRight="10dip" android:layout_marginTop="10dip"></Button>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout2"
		android:layout_height="wrap_content" android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true" android:layout_width="fill_parent">
		<Button android:drawableTop="@drawable/ok_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/profile_update"
			android:text="UPDATE" android:layout_height="wrap_content"
			android:layout_width="0dp"></Button>
		<Button android:drawableTop="@drawable/reset_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/profile_cancel"
			android:text="CANCEL" android:layout_height="wrap_content"
			android:layout_width="0dp"></Button>
		<Button android:drawableTop="@drawable/exit_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/profile_logout"
			android:text="LOGOUT" android:layout_height="wrap_content"
			android:layout_width="0dp"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/ll1_profile"
		android:layout_below="@+id/linearLayout1"
		android:layout_alignParentLeft="true">
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_weight="1"
			android:layout_width="0dp" android:layout_marginTop="20dp"
			android:layout_marginLeft="25dp" android:textAppearance="?android:attr/textAppearanceMedium"
			android:id="@+id/tv_pwelcome" android:text="Welcome :"></TextView>
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_weight="1"
			android:layout_width="0dp" android:layout_marginTop="20dp"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:id="@+id/tv_puname"></TextView>
	</LinearLayout>
	<ScrollView android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@+id/scroll_profile"
		android:layout_below="@+id/ll1_profile"
		android:layout_alignParentLeft="true" android:layout_marginBottom="50dp"
		android:layout_marginTop="5dp">
		<LinearLayout android:id="@+id/ll2_profile"
			android:orientation="vertical" android:layout_width="fill_parent"
			android:layout_height="fill_parent">

			<LinearLayout android:id="@+id/ll3_profile"
				android:orientation="vertical" android:layout_width="fill_parent"
				android:layout_height="fill_parent">
				<TextView android:text="Personal Information"
					android:textColor="#000000" android:id="@+id/tv_ppersonal"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:textStyle="bold" android:textSize="16px"></TextView>
				<LinearLayout android:id="@+id/ll4_profile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Username" android:textColor="#000000"
						android:id="@+id/tv_puname" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_uname"
						android:layout_weight="1" android:editable="false"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll5_profile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Password" android:id="@+id/tv_ppwd"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_pwd"
						android:layout_weight="1" android:inputType="textPassword">
						<requestFocus></requestFocus>
					</EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll6_profile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Confirm Password" android:id="@+id/tv_pcpwd"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_cpwd"
						android:layout_weight="1" android:inputType="textPassword"></EditText>
				</LinearLayout>



			</LinearLayout>

			<LinearLayout android:id="@+id/ll9_profile"
				android:layout_marginTop="5dp" android:orientation="vertical"
				android:layout_width="fill_parent" android:layout_height="fill_parent">
				<TextView android:text="Contact Information"
					android:textColor="#000000" android:id="@+id/tv_pcontact"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:textStyle="bold" android:textSize="16px"></TextView>

				<LinearLayout android:id="@+id/ll10_profile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="E-mail Id" android:textColor="#000000"
						android:id="@+id/tv_peid" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_eid"
						android:layout_weight="1" android:inputType="textEmailAddress"></EditText>
				</LinearLayout>

				<LinearLayout android:id="@+id/ll11_profile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Phone No." android:textColor="#000000"
						android:id="@+id/tv_pph" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_ph"
						android:layout_weight="1" android:inputType="number"></EditText>
				</LinearLayout>

				<LinearLayout android:id="@+id/ll12_profile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="City" android:id="@+id/tv_pcity"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/profile_cityspinner"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>


				<LinearLayout android:id="@+id/ll14_profile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Zip Code" android:id="@+id/tv_pzip"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_zip"
						android:layout_weight="1" android:inputType="number"></EditText>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>

	</ScrollView>


</RelativeLayout>
