<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:background="@drawable/contact_us_title"
		android:id="@+id/linearLayout1" android:layout_height="wrap_content"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_width="fill_parent" android:layout_marginBottom="10dip">
		<Button android:drawableLeft="@drawable/home"
			android:background="@null" android:textColor="#ffffff" android:id="@+id/contact_home"
			android:layout_height="wrap_content" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:layout_marginBottom="5dip"
			android:layout_weight="1" android:layout_width="0dp"></Button>
		<TextView android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_height="wrap_content" android:id="@+id/textView4"
			android:text=" " android:layout_weight="1" android:layout_width="0dp"></TextView>
		<Button android:drawableRight="@drawable/back_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/contact_back"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:layout_marginRight="10dip" android:layout_marginTop="10dip"></Button>
	</LinearLayout>
	<ScrollView android:layout_height="wrap_content"
		android:layout_marginTop="50dp" android:layout_width="wrap_content"
		android:id="@+id/scroll_contact" android:layout_alignParentLeft="true"
		android:layout_below="@+id/ll_contact" android:layout_marginBottom="50dp"
		android:layout_alignParentRight="true" android:layout_alignParentTop="true">
		<LinearLayout android:id="@+id/ll1_contact"
			android:orientation="vertical" android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			<TextView android:textAppearance="?android:attr/textAppearanceMedium"
				android:id="@+id/tv_add" android:layout_width="wrap_content"
				android:text="Address" android:textStyle="bold" android:textColor="#000000"
				android:layout_height="wrap_content"></TextView>
			<TextView android:id="@+id/tv_addv" android:layout_width="wrap_content"
				android:textColor="#000000" android:layout_height="wrap_content"></TextView>
			<TextView android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="#000000" android:id="@+id/tv_ph"
				android:layout_width="wrap_content" android:text="Phone"
				android:textStyle="bold" android:layout_height="wrap_content"></TextView>
			<TextView android:id="@+id/tv_phv" android:layout_width="wrap_content"
				android:textColor="#000000" android:layout_height="wrap_content"></TextView>
			<TextView android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="#000000" android:id="@+id/tv_fax"
				android:layout_width="wrap_content" android:text="Fax"
				android:textStyle="bold" android:layout_height="wrap_content"></TextView>
			<TextView android:id="@+id/tv_faxv" android:layout_width="wrap_content"
				android:textColor="#000000" android:layout_height="wrap_content"></TextView>
			<TextView android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="#000000" android:id="@+id/tv_eid"
				android:layout_width="wrap_content" android:text="E-mail"
				android:textStyle="bold" android:layout_height="wrap_content"></TextView>
			<TextView android:id="@+id/tv_eidv" android:layout_width="wrap_content"
				android:textColor="#000000" android:layout_height="wrap_content"></TextView>
			<TextView android:textAppearance="?android:attr/textAppearanceMedium"
				android:textColor="#000000" android:id="@+id/tv_site"
				android:layout_width="wrap_content" android:text="Website"
				android:textStyle="bold" android:layout_height="wrap_content"></TextView>
			<TextView android:id="@+id/tv_sitev" android:layout_width="wrap_content"
				android:textColor="#000000" android:layout_height="wrap_content"></TextView>
		</LinearLayout>
	</ScrollView>
</RelativeLayout>
