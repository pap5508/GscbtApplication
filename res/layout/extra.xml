<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:background="@drawable/profile_title"
		android:id="@+id/linearLayout1" android:layout_height="wrap_content"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_width="fill_parent" android:orientation="horizontal">
		<Button android:drawableLeft="@drawable/home"
			android:background="@null" android:textColor="#ffffff" android:id="@+id/profile_homebb"
			android:layout_height="wrap_content" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:layout_marginBottom="5dip"
			android:layout_weight="1" android:layout_width="0dp"></Button>
		<TextView android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_height="wrap_content" android:id="@+id/textView4"
			android:text=" " android:layout_weight="1" android:layout_width="0dp"></TextView>
		<Button android:drawableRight="@drawable/back_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/donor_back"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:layout_marginRight="10dip" android:layout_marginTop="10dip"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:id="@+id/ll1_bbprofile" android:layout_height="wrap_content"
		android:layout_below="@+id/linearLayout1"
		android:layout_alignParentLeft="true">
		<TextView android:layout_weight="1" android:layout_marginTop="20dp"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="#000000" android:layout_height="wrap_content"
			android:layout_width="0dp" android:text="Welcome" android:id="@+id/tv_bbpwelcome"></TextView>
		<TextView android:layout_weight="1" android:layout_marginTop="20dp"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:id="@+id/tv_bbpuname"></TextView>
	</LinearLayout>

	<ScrollView android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@+id/scroll_bbprofile"
		android:layout_below="@+id/ll1_bbprofile"
		android:layout_alignParentLeft="true" android:layout_marginBottom="50dp"
		android:layout_marginTop="5dp">
		<LinearLayout android:id="@+id/ll2_bbprofile"
			android:orientation="vertical" android:layout_width="fill_parent"
			android:layout_height="fill_parent">

			<LinearLayout android:id="@+id/ll4_bbprofile"
				android:orientation="vertical" android:layout_width="fill_parent"
				android:layout_height="fill_parent">
				<TextView android:text="BloodBank Information" android:id="@+id/tv_personalbbp"
					android:textColor="#000000" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textSize="24px"
					android:textStyle="bold"></TextView>

				<LinearLayout android:id="@+id/ll6_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Username" android:id="@+id/tv_unamebbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_unamebb"
						android:layout_weight="1"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll7_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Password" android:id="@+id/tv_pwdbbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_pwdbb"
						android:layout_weight="1" android:inputType="textPassword"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll8_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Confirm Password" android:id="@+id/tv_cpwdbbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_cpwdbb"
						android:layout_weight="1" android:inputType="textPassword"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll9_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Bloodbank Category" android:id="@+id/tv_categorybbp"
						android:layout_width="0dp" android:layout_height="wrap_content"
						android:textColor="#000000" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/profile_spinnercatbb"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll10_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Bloodbank SubCategory" android:id="@+id/tv_subcatbbp"
						android:layout_width="0dp" android:layout_height="wrap_content"
						android:textColor="#000000" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/profile_spinnersubcatbb"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll11_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="NACO Approved" android:id="@+id/tv_nacobbp"
						android:layout_width="0dp" android:layout_height="wrap_content"
						android:textColor="#000000" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/profile_spinnernacobb"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>

			</LinearLayout>




			<LinearLayout android:id="@+id/ll13_bbprofile"
				android:layout_marginTop="5dp" android:orientation="vertical"
				android:layout_width="fill_parent" android:layout_height="fill_parent">
				<TextView android:text="Contact Information" android:id="@+id/tv_contactbbp"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:textColor="#000000" android:textSize="24px"
					android:textStyle="bold"></TextView>

				<LinearLayout android:id="@+id/ll14_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Address" android:id="@+id/tv_addbbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_addbb"
						android:layout_weight="1" android:inputType="textMultiLine"></EditText>
				</LinearLayout>

				<LinearLayout android:id="@+id/ll15_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="City" android:id="@+id/tv_citybbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/profile_spinnercitybb"
						android:layout_width="0dp" android:layout_weight="1"
						android:layout_height="40dp"></Spinner>
				</LinearLayout>

				<LinearLayout android:id="@+id/ll16_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="State" android:id="@+id/tv_statebbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_statebb"
						android:layout_weight="1"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll17_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Zip Code" android:id="@+id/tv_zipbbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_zipbb"
						android:layout_weight="1" android:inputType="number"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll18_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="Phone Number " android:id="@+id/tv_phbbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_phbb"
						android:layout_weight="1" android:inputType="number"></EditText>
				</LinearLayout>
				<LinearLayout android:id="@+id/ll19_bbprofile"
					android:layout_width="fill_parent" android:layout_height="wrap_content">
					<TextView android:text="E-mail" android:id="@+id/tv_eidbbp"
						android:textColor="#000000" android:layout_width="0dp"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:layout_height="40dp"
						android:layout_width="0dp" android:id="@+id/profile_eidbb"
						android:layout_weight="1" android:inputType="textEmailAddress"></EditText>
				</LinearLayout>

			</LinearLayout>
		</LinearLayout>
	</ScrollView>
	<LinearLayout android:background="@drawable/footer_title"
		android:layout_width="fill_parent" android:id="@+id/linearLayout4"
		android:layout_height="wrap_content" android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true">
		<Button android:layout_weight="1" android:drawableTop="@drawable/ok_icon"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:textColor="#ffffff" android:id="@+id/profile_submitbb"
			android:text="UPDATE" android:background="@null"
			android:layout_gravity="bottom"></Button>
		<Button android:layout_weight="1" android:drawableTop="@drawable/reset_icon"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:textColor="#ffffff" android:id="@+id/profile_cancelbb"
			android:text="RESET" android:background="@null"
			android:layout_gravity="bottom"></Button>
		<Button android:layout_weight="1" android:drawableTop="@drawable/exit_icon"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:textColor="#ffffff" android:id="@+id/bbprofile_logout"
			android:text="LOGOUT" android:background="@null"
			android:layout_gravity="bottom"></Button>
	</LinearLayout>
</RelativeLayout>
