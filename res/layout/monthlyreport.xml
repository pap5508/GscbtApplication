<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:background="@drawable/reports_title"
		android:id="@+id/linearLayout1" android:layout_height="wrap_content"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_width="fill_parent" android:orientation="horizontal">
		<Button android:drawableLeft="@drawable/home"
			android:background="@null" android:textColor="#ffffff" android:id="@+id/report_home"
			android:layout_height="wrap_content" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:layout_marginBottom="5dip"
			android:layout_weight="1" android:layout_width="0dp"></Button>
		<TextView android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_height="wrap_content" android:id="@+id/textView4"
			android:text=" " android:layout_weight="1" android:layout_width="0dp"></TextView>
		<Button android:drawableRight="@drawable/back_icon"
			android:background="@null" android:textColor="#ffffff"
			android:layout_weight="1" android:id="@+id/report_back"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:layout_marginRight="10dip" android:layout_marginTop="10dip"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/ll1_report"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_marginTop="54dp">
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_width="wrap_content"
			android:layout_marginTop="20dp" android:textAppearance="?android:attr/textAppearanceMedium"
			android:id="@+id/tv_reportwelcome" android:text="Welcome"></TextView>
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_width="wrap_content"
			android:layout_marginTop="20dp" android:layout_marginLeft="10dp"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:id="@+id/tv_reportuname" android:text="User"></TextView>
	</LinearLayout>
	<LinearLayout android:background="@drawable/footer_title"
		android:layout_height="wrap_content" android:id="@+id/linearLayout2"
		android:orientation="vertical" android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true" android:layout_width="fill_parent">
		<Button android:drawableTop="@drawable/exit_icon"
			android:background="@null" android:textColor="#ffffff" android:text="LOGOUT"
			android:id="@+id/report_logout" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:layout_gravity="bottom"
			android:layout_marginTop="10dip"></Button>
	</LinearLayout>

</RelativeLayout>
