<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/back"
	android:id="@+id/relativeLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:gravity="top">
	<LinearLayout android:layout_width="fill_parent"
		android:background="@drawable/search_title" android:layout_height="wrap_content"
		android:id="@+id/linearLayout1" android:orientation="horizontal"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true">
		<Button android:id="@+id/search1_home" android:drawableLeft="@drawable/home"
			android:layout_height="wrap_content" android:textColor="#ffffff"
			android:layout_marginBottom="5dip" android:layout_weight="1"
			android:layout_width="0dp" android:layout_gravity="left"
			android:layout_marginLeft="10dip" android:background="@null"></Button>
		<TextView android:layout_height="wrap_content"
			android:textColor="#000000" android:layout_weight="1"
			android:layout_width="0dp" android:textAppearance="?android:attr/textAppearanceMedium"
			android:id="@+id/textView4" android:text=" "></TextView>
		<Button android:id="@+id/search1_back"
			android:layout_marginRight="10dip" android:layout_height="wrap_content"
			android:textColor="#ffffff" android:layout_weight="1"
			android:layout_width="0dp" android:layout_marginTop="10dip"
			android:drawableRight="@drawable/back_icon" android:background="@null"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/linearLayout2"
		android:layout_below="@+id/linearLayout1"
		android:layout_alignParentLeft="true" android:layout_marginTop="10dip">
		<TextView android:layout_marginRight="5dip"
			android:textColor="#000000" android:layout_height="wrap_content"
			android:layout_weight="1" android:layout_width="wrap_content"
			android:textStyle="bold" android:layout_marginLeft="5dip" android:id="@+id/text1"
			android:text="BloodBank"></TextView>
		<TextView android:layout_marginRight="2dip"
			android:textColor="#000000" android:layout_height="wrap_content"
			android:layout_weight="1" android:layout_width="wrap_content"
			android:textStyle="bold" android:layout_marginLeft="60dip"
			android:id="@+id/text2" android:text="WholeBlood"></TextView>
		<TextView android:layout_weight="1" android:textColor="#000000"
			android:layout_marginRight="2dip" android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:textStyle="bold"
			android:layout_marginLeft="2dip" android:id="@+id/textView1"
			android:text="Map"></TextView>
		<TextView android:layout_weight="1" android:textColor="#000000"
			android:layout_marginRight="0dip" android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:textStyle="bold"
			android:layout_marginLeft="2dip" android:id="@+id/textView2"
			android:text="Detail"></TextView>
	</LinearLayout>
	<ListView android:id="@+id/search1list" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_below="@+id/linearLayout2"
		android:layout_alignParentLeft="true"></ListView>
</RelativeLayout>
